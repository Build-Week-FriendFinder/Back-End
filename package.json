{
  "name": "friend-finder-back-end",
  "version": "1.0.0",
  "description": "Build Week Project for Back-End Unit 4",
  "main": "index.js",
  "scripts": {
    "server": "nodemon index.js",
    "start": "node index.js",
    "test": "cross-env DB_ENV=testing jest --watch --verbose",
    "rollback": "knex migrate:rollback --env production",
    "migrate": "knex migrate:latest --env production",
    "seed": "knex seed:run --env production",
    "api-docs": "apidoc -f .js -i ./api -o apidoc/",
    "build-docs": "apidoc-markdown -p ./apidoc -o README.md",
    "build-api": "npm-run-all api-docs build-docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Build-Week-FriendFinder/Back-End"
  },
  "keywords": [],
  "author": "Levi Thomas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Build-Week-FriendFinder/Back-End/issues"
  },
  "homepage": "https://github.com/Build-Week-FriendFinder/Back-End#readme",
  "dependencies": {
    "apidoc": "^0.17.7",
    "apidoc-markdown": "^0.2.1",
    "body-parser": "^1.19.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.20.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.2",
    "knex-cleaner": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "pg": "^7.12.1",
    "minipass": "^2.7.0",
    "sqlite3": "^4.1.0"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2",
    "cross-env": "^6.0.0"
  }
}
